{
  "ci": {
    "collect": {
      "url": ["https://liew-liew.github.io/devops-test/"],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["error", { "minScore": 0.8 }],
        "categories:performance": ["warn", { "minScore": 0.7 }]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci-report"
    }
  }
}
